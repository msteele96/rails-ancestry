<h1>Log In</h1>
<%= form_tag('/login', method: :post) do %>
    <%=label_tag(:name)%>
    <%=text_field_tag(:name)%>
    <%=label_tag(:password)%>
    <%=password_field_tag(:password)%>
    <%=submit_tag("Log in")%>
<% end %>
